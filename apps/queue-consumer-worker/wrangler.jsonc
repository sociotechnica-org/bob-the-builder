{
  "name": "bob-queue-consumer-worker",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-07",
  "compatibility_flags": ["nodejs_compat"],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "bob-control-worker",
      "database_id": "00000000-0000-0000-0000-000000000000",
      "migrations_dir": "../control-worker/migrations"
    }
  ],
  "queues": {
    "consumers": [
      {
        "queue": "bob-runs"
      }
    ]
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "vars": {
    "LOCAL_QUEUE_SHARED_SECRET": "",
    "CODERUNNER_MODE": "mock",
    "MODAL_TOKEN_ID": "",
    "MODAL_TOKEN_SECRET": "",
    "CLAUDE_CODE_API_KEY": ""
  }
}
